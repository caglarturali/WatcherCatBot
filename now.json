{
  "version": 2,
  "name": "watchercatbot",
  "alias": "watchercatbot.now.sh",
  "builds": [{ "src": "*.js", "use": "@now/node" }],
  "env": {
    "TELEGRAM_TOKEN": "@telegram_token",
    "APP_URL": "https://watchercatbot.now.sh",
    "PORT": "443"
  },
  "routes": [{ "src": "/(.*)", "dest": "bot.js" }]
}
